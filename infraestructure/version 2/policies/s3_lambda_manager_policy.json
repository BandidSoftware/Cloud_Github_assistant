{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "s3:GetObject",
          "s3:PutObject",
          "sqs:SendMessage"
        ],
        "Resource": [
          "arn:aws:s3:::code-files/*",
          "arn:aws:s3:::code-files",
          "arn:aws:sqs:REGION:ACCOUNT_ID:queue_databases"
        ]
      }
    ]
  }
  