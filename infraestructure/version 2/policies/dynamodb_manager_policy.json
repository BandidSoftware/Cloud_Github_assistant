{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "sqs:SendMessage",
          "sqs:ReceiveMessage",
          "sqs:DeleteMessage",
          "dynamodb:Scan",
          "dynamodb:GetItem",
          "dynamodb:PutItem",
          "dynamodb:UpdateItem",
          "dynamodb:DeleteItem"
        ],
        "Resource": [
          "arn:aws:sqs:REGION:ACCOUNT_ID:TU_COLA_SQS",
          "arn:aws:dynamodb:REGION:ACCOUNT_ID:table/TU_TABLA_DYNAMODB"
        ]
      }
    ]
  }
  